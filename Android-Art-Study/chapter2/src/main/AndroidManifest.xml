<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.changxin.chapter2">


    <permission android:name="com.cx.aidl.chapter2"
                android:protectionLevel="normal"/>
    <uses-permission android:name="com.cx.aidl.chapter2"/>


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:process=":remote" />
        <activity
            android:name=".ThirdActivity"
            android:process="com.changxin.art" />

        <service android:name=".Service.MyService" />
        <service
            android:name=".Messenger.MessengerService"
            android:process=":real" />

        <activity android:name=".Messenger.MessengerActivity"></activity>
    </application>

</manifest>